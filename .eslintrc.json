{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-alert": "off",
        "radix": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "func-names": "off",
        "no-empty-function": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
